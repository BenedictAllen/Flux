import "parsing classes";

// changed:

let roots(A, B, C) = [(-B + sqrt(disc)) / 2A, (-B - sqrt(disc)) / 2A]
	where disc = B ** 2 - 4 * A * C
	where 2A = 2 * A;

let expand(A, B, C, D) = [A * C, A * D + B * C, B * D];

void main() {
	// (x + 2)(4x + 3)
	let coeff = expand( 1, 2, 4, 3 );
	let r = roots( coeff[1], coeff[2], coeff[3] );

	foreach root in r
		print( r ); // -2, -3/4
}


namespace Flux::Parsing;

class BacktickExpression extends RightUnaryExpression {
	Expression lvalue, rvalue;
	string op;

	BacktickExpression( null Expression lvalue, null Expression rvalue, string op, null Position position = (lvalue || rvalue).position ) {
		self.lvalue = lvalue;
		self.rvalue = rvalue;
		self.op = op;
		self.position = position;
	}
}


namespace Flux::Parsing;

class Cast extends RightUnaryExpression {
	ASTNodeType type = Flux::Parsing::ASTNodeType.Cast;
	Expression value;
	Type _type;

	Cast( Expression value, Type type, null Position position = value.position ) {
		self.value = value;
		self._type = type;
		self.position = position;
	}
}

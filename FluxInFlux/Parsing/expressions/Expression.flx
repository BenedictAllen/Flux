
import PrimaryExpression;
import RightUnaryExpression;
import LeftUnaryExpression;
import BacktickExpression;
import BinaryExpression;
import operators.TernaryOperatorParser;

namespace Flux::Parsing;

class Expression extends ASTNode {
	static ASTNode parse(Source source);
}

ASTNode Expression:parse(Source source) {
	new Flux::Parsing::TernaryOperatorParser expr();
	return expr:parse( source );
}


import types.Type;
import definitions.Definition;
import ASTNode;

namespace Flux::Parsing;

class File {
	string name;

	File[] imports;

	Type[] types;
	Definition[] definitions;
	ASTNode statements;

	File(string name) {
		self.name = name;
		self.imports = [];
		self.types = [];
		self.definitions = [];
		self.statements = [];
	}

	void add_import(File);

	void define_type(Type);
	void define(Definition);
	void push(statement);
}

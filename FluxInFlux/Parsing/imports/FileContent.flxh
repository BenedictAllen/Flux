
namespace Flux::Parsing;

class FileContent {
	string name;
	ClassDefinition[] classes;
	ASTNode[] statements;
	FileContent header;

	FileContent(string name, null FileContent header) {
		self.name = name;
		self.classes = [];
		self.header = header;
		self.statements = [];
	}

	void addClassDefinition(ClassDefinition def);
	void push(ASTNode statement);
}

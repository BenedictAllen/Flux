
namespace Flux::Parsing;

class EnumDefinition extends ASTNode {
	ASTNodeType type = Flux::Parsing::ASTNodeType.EnumDefinition;
	string name;
	string[] members;

	EnumDefinition(string name, string[] members, Position position) {
		self.name = name;
		self.members = members;
		self.position = position;
	}
}

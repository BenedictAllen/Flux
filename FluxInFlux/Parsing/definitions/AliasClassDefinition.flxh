
import ClassMember;

namespace Flux::Parsing;

class AliasClassDefinition extends Flux::Parsing::ASTNode {
	Type name;
	TemplateList _template;
	Type[] types;

	AliasClassDefinition(Type name, TemplateList _template, Type[] types, Position position) {
		self.name = name;
		self._template = _template;
		self.types = types;
		self.position = position;
	}
}

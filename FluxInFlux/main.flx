
import flux;

let source_code = "
new Button(1, 1, 5, 8, \"Hello\")
new int[](5)
new vector<int>(2)
match !true {
	yes => this;
	no, maybe => that;
	default => abc
}
";

void main() {
	new Flux::Parsing::Source source();
	new Flux::Parsing::Lexer lexer( source_code );

	source.lexer = lexer;

	while !lexer:isEOF() {
		let expr = Flux::Parsing::Expression:parse( source );
		if expr
			print( expr:serialize() );
		else {
			print( "Nope" );
			break;
		}
	}
}


import flux;

let source_code = "
T<A>[]
function float(int)[]{int}
";

void main() {
	new Flux::Parsing::Source source();
	new Flux::Parsing::Lexer lexer( source_code );

	source.lexer = lexer;

	while !lexer:isEOF() {
		let expr = Flux::Parsing::Type:parse( source );
		if expr
			print( expr:serialize() );
		else {
			print( "Nope" );
			break;
		}
	}
}

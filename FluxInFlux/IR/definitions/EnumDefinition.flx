
import Definition;

namespace Flux::IR;

class EnumDefinition extends Flux::IR::Definition {
	string[] members;

	EnumDefinition(bool is_public, string name, string[] members) {
		self.members = members;
	}

	bool has_index(string name);
}

bool EnumDefinition:has_index(string name) {
	foreach member in self.members
		if name == member
			return true;

	return false;
}

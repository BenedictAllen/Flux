
import IRDefinition;
import types.IRTypeSignature;
import Position;

class IRObjectDefinition extends IRDefinition {
	bool isConst;
	Position position;
	string[] flags;

	IRObjectDefinition(string name, bool isConst, isPublic, IRTypeSignature type, string[] flags, Position position) {
		self.name = name;
		self.isConst = isConst;
		self.isPublic = isPublic;
		self.type = type;
		self.flags = flags;
		self.position = position;
	}
}

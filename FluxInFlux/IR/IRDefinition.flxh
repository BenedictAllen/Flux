
// namespace Flux::Compiling;

import IRType;

class IRDefinition {
	string name;
	bool isConst;
	IRType type;
	Position position;

	IRDefinition(string name, bool isConst, IRType type, Position position) {
		self.name = name;
		self.isConst = isConst;
		self.type = type;
		self.position = position;
	}
}


IRReference IRReferenceList:select( int index ) {
	return new IRReference( self.list[index], self.types[index] );
}

string IRReferenceList:tostring() {
	string[] s = [];

	foreach i, v in self.list
		s[i] = v:definition().name;

	return "ReferenceList[" `concat` table.concat( s, ", " ) `concat` "]";
}


import Expression;
import Node;

namespace Flux::IR;

class NativeLeftUnaryOperatorExpression extends Flux::IR::Expression {
	Node operand;
	string op;

	NativeLeftUnaryOperatorExpression(Node operand, string op) {
		self.operand = operand;
		self.op = op;
	}
}

string NativeLeftUnaryOperatorExpression:serialize()
	= "nativeunl(" `concat` self.op
	`concat` "," `concat` self.operand:serialize()
	`concat` ")";
